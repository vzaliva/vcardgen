(jbuild_version 1)

(library (
          (name vcardgen)
          (public_name vcardgen)
          (libraries (batteries))
          (modules (vcardgen))
          ))

(executable
 ((name example)
  (libraries (batteries))
  (modules (example))
  ))

(install
 ((section doc)
  (files (example.ml botero.jpeg))))




